{
    "appName": "${project.artifactId}",
    "profiles": [ ],
    "jdbc": {
        "jdbc/work-presentation": {
            "url": "${WORK_PRESENTATION_POSTGRES_URL}",
            "maxSize": "${MAX_POOL_SIZE}"
        }
    },
    "hazelcast": {
        "clusterName": "${project.parent.artifactId}-${SYSTEM_NAME}",
        "cacheConfig": {
            "*": {
                "timeout": "10",
                "timeUnit": "Minutes"
            },
            "vip-core": {
                "timeout": "3",
                "timeUnit": "Hours"
            },
            "vip-core-error": {
                "timeout": "3",
                "timeUnit": "Minutes"
            }
        }
    }
}
